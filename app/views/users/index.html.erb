<h1>Listing users</h1>

<table border="1" >
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>

<br />
<%= will_paginate %>
<%= link_to 'home page', root_path %>
<%= link_to 'calendar', calendar_path %>